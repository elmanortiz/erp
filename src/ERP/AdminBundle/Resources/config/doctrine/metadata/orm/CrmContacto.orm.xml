<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrmContacto" table="crm_contacto">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="true"/>
    <field name="apellido" type="string" column="apellido" length="100" nullable="true"/>
    <field name="telefono" type="string" column="telefono" length="20" nullable="true"/>
    <field name="emailId" type="string" column="email_id" length="50" nullable="true"/>
    <field name="numeroMovil" type="string" column="numero_movil" length="20" nullable="true"/>
    <field name="departamento" type="string" column="departamento" length="50" nullable="true"/>
    <field name="puesto" type="string" column="puesto" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
