<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlImpuesto" table="ctl_impuesto">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="impNombre" type="string" column="imp_nombre" length="200" nullable="false"/>
    <field name="impPorcentaje" type="float" column="imp_porcentaje" precision="10" scale="0" nullable="false"/>
    <field name="impEstado" type="integer" column="imp_estado" nullable="false"/>
  </entity>
</doctrine-mapping>
