<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlEmpresa" table="ctl_empresa">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreEmpresa" type="string" column="nombre_empresa" length="45" nullable="true"/>
    <field name="nrc" type="string" column="nrc" length="45" nullable="false"/>
    <field name="nit" type="string" column="nit" length="45" nullable="false"/>
    <field name="giro" type="string" column="giro" length="45" nullable="false"/>
  </entity>
</doctrine-mapping>
