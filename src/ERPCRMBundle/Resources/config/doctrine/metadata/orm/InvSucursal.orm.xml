<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="InvSucursal" table="inv_sucursal">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false"/>
    <field name="alias" type="string" column="alias" length="50" nullable="true"/>
    <field name="direccion1" type="string" column="direccion1" length="200" nullable="false"/>
    <field name="direccion2" type="string" column="direccion2" length="200" nullable="true"/>
    <field name="ciudad" type="string" column="ciudad" length="100" nullable="false"/>
    <field name="provinciaEstado" type="string" column="provincia_estado" length="25" nullable="false"/>
    <field name="postalZip" type="string" column="postal_zip" length="10" nullable="true"/>
    <field name="estado" type="boolean" column="estado" nullable="true"/>
  </entity>
</doctrine-mapping>
