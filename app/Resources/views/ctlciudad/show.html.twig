{% extends 'base.html.twig' %}

{% block body %}
    <h1>CtlCiudad</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ctlCiudad.id }}</td>
            </tr>
            <tr>
                <th>Nombreciudad</th>
                <td>{{ ctlCiudad.nombreCiudad }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('ctlciudad_index') }}">Back to the list</a>
